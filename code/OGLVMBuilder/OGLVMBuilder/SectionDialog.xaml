<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="OGLVMBuilder.SectionDialog"
        Title="Add state" Width="179" Height="152" WindowStyle="ToolWindow" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterScreen" Topmost="True">
    <StackPanel>
        <TextBlock Text="Please enter the name:" Margin="8" Name="Box"/>
        <TextBox Margin="8" MaxLength="16" x:Name="Text" PreviewKeyDown="Text_PreviewKeyDown"/>
        <StackPanel Orientation="Horizontal" Margin="8" HorizontalAlignment="Right">
            <Button Content="Ok" x:Name="Ok" Margin="8" Padding="8,2" Click="Ok_Click" IsDefault="True"/>
            <Button Content="Cancel" x:Name="Cancel" Margin="8" Padding="8,2" Click="Cancel_Click" IsCancel="True"/>
        </StackPanel>
    </StackPanel>
</Window>
